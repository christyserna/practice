

#include <stdio.h>
#include <string.h>

int main() {


char s1[] = "Apple";
char s2[] = "Applet";

int res = strcmp(s1, s2);

if (res == 0) 
  printf("string 1 & string 2 are the same.");
else if ( res == < 0)
  printf("s1 is smaller than s2");
else
  printf("string1 is bigget than string 2!");

return 0;
}


void printStr(char str[]) {
  printf("%s", str);
}

int main() {
char str[] = "GLHF";

printStr(str);
return 0;
}



int main() {
char s[] = "abcdef";

printf("%s", strlen(s));
return 0;
}



int main() {
char str[] = "Hello!";

char dest[20];

strcpy(dest, str);
printf("%s", dest);
return 0;
}



int main() {

char s1[30] = "Hello, ";
char s2[] = "Students!";

strcat(s1, s2);
printf("%s", s1


return 0;
}



